{
  "name": "focusbook",
  "version": "1.0.0",
  "description": "Focusbook",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "build-ai-service": "cd AI_agent && python build_service.py",
    "prebuild": "npm run build-ai-service",
    "postinstall": "electron-builder install-app-deps && npm run install-python-deps",
    "install-python-deps": "python -m pip install -r requirements.txt && cd AI_agent && python -m pip install -r requirements.txt",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "setup": "npm install && python -m pip install -r requirements.txt"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
